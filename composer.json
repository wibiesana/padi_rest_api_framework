{
    "name": "wibiesana/padi-rest-api",
    "description": "Padi REST API Framework: Grow Silent, Feed Many. Lightweight & Scalable.",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/wibiesana/padi_rest_api-main",
    "support": {
        "issues": "https://github.com/wibiesana/padi_rest_api-main/issues",
        "source": "https://github.com/wibiesana/padi_rest_api-main"
    },
    "require": {
        "php": ">=8.4",
        "wibiesana/padi-core": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wibiesana/padi_core.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Wibiesana",
            "email": "wibiesana@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}